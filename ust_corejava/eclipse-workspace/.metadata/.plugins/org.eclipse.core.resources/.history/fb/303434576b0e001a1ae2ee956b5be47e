package com.ustglobal.exceptionHandling;

public class Demo1 {

	public static void main(String[] args) {

		try {
			Thread.sleep(3000);
			System.out.println("inside try");
		}catch(Exception ie) 
		{

               System.out.println("exception caught");
               
               ie.printStackTrace();

		}
		
		System.out.println("main ended");
	}

}
