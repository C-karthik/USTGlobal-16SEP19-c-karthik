public class TestP {
	public static void main(String[] args) {
		
		Person p = new Person();
		p.age = 21;
		p.name = "vijay";
		
		System.out.println(p.age);
		System.out.println(p.name);
		
		System.out.println("============= static varibles and method=========");
		
		
		Person.color = "fair";
		
		System.out.println(Person.color);
		Person.walk();
		
	}

}