package com.ustglobal.jpawithibernateapp.onetmany;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;

@Data
@Entity
@Table(name = "pencil")
public class Pencil {

	@Column
	@Id
	private int price;
	
	@Column
	private String brand;
	
	
	
}
