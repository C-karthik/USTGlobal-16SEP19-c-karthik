package com.ustglobal.jpawithibernateapp.jpql.manytomany;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table( name = "course")
public class Course {

	
	@Id
	@Column
	private int cid;
	
	@Column
	private int cname;
	
	
	private Student student ;
	
	
	
	
	
}
