<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>webap</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<context-param>

		<param-name>company-name</param-name>
		<param-value>tyss</param-value>

	</context-param>

	<servlet>

		<servlet-name>firstServlet</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.MyFirstServlets</servlet-class>

		<init-param>

			<param-name>batch-name</param-name>
			<param-value>ust</param-value>

		</init-param>

	</servlet>

	<servlet-mapping>

		<servlet-name>firstServlet</servlet-name>
		<url-pattern>/first</url-pattern>

	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>formServlet</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.FormServlets</servlet-class>

		<init-param>

			<param-name>batch-name</param-name>
			<param-value>ust_global</param-value>

		</init-param>

	</servlet>

	<servlet-mapping>

		<servlet-name>formServlet</servlet-name>
		<url-pattern>/form</url-pattern>

	</servlet-mapping>

	<servlet>

		<servlet-name>redirect</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.RedirectiveServlets</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>redirect</servlet-name>
		<url-pattern>/redirect</url-pattern>

	</servlet-mapping>

	<servlet>

		<servlet-name>forward</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.ForwardServlets</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>forward</servlet-name>
		<url-pattern>/forward</url-pattern>

	</servlet-mapping>

	<servlet>

		<servlet-name>include</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.IncludeServlet</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>include</servlet-name>
		<url-pattern>/include</url-pattern>

	</servlet-mapping>

	<servlet>

		<servlet-name>formForward</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.FormForwordServlet</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>formForward</servlet-name>
		<url-pattern>/form-forward</url-pattern>

	</servlet-mapping>

	<servlet>

		<servlet-name>createCookie</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.CreateCookieServlet</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>createCookie</servlet-name>
		<url-pattern>/createCookie</url-pattern>

	</servlet-mapping>

	<servlet>

		<servlet-name>readCookie</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.ReadCookieServlet</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>readCookie</servlet-name>
		<url-pattern>/readCookie</url-pattern>

	</servlet-mapping>
	
	<servlet>

		<servlet-name>deleteCookie</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.DeleteCookieServlet</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>deleteCookie</servlet-name>
		<url-pattern>/deleteCookie</url-pattern>

	</servlet-mapping>
	

	<servlet>

		<servlet-name>contextAttribute</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.SetContextAttribute</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>contextAttribute</servlet-name>
		<url-pattern>/contextAttribute</url-pattern>

	</servlet-mapping>

	<servlet>

		<servlet-name>getcontextAttribute</servlet-name>
		<servlet-class>com.ustglobal.webap.servlets.GetContextAttributeServlets</servlet-class>

	</servlet>


	<servlet-mapping>

		<servlet-name>getcontextAttribute</servlet-name>
		<url-pattern>/getcontextAttribute</url-pattern>

	</servlet-mapping>





</web-app>
